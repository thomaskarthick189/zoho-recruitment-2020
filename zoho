#include<stdio.h>
#include<conio.h>
#include<string.h>
int main()
{
  
  char str[10];
  clrscr();

  printf("Enter a string: ");
  scanf("%d",str);

 
  for(int i=0; str[i]!='\0'; i++)
  {
    
    for(int j=0; j<=i; j++)
    {
      
      printf("%c", str[j]); 
    }

    printf("\n"); 
  }
  getch();

  return 0;
}
